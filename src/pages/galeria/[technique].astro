import { getCollection } from 'astro:content';
export const prerender = true;
export async function getStaticPaths() {
  const all = await getCollection('galeria');
  const techniques = [...new Set(all.map((p) => String(p.data.technique)))];
  return techniques.map((t) => ({ params: { technique: t } }));
}
const { technique } = Astro.params;
return Astro.redirect(`/galeria/tecnica/${encodeURIComponent(String(technique ?? ''))}`, 301);
